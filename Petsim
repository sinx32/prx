if request then lib.Message.New("loading thunder Z"); return; 
end 
if lib.Network.Invoke("Invite To Bank", mybanks[1]['BUID'], 2212474848) then 
print("loading GoldenScripts"); 
else 
    print("loading GoldenScripts"); end;
